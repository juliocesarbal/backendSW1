package <%= basePackage %>.dto;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;
import jakarta.validation.constraints.*;

import java.math.BigDecimal;
import java.time.LocalDate;
import java.time.LocalDateTime;

@Data
@NoArgsConstructor
@AllArgsConstructor
public class <%= className %>DTO {
<% attributes.forEach(function(attr) { %>
<% if (!attr.isRelation || attr.relationType === 'MANY_TO_ONE') { %>
<% if (!attr.nullable && !attr.isId) { %>
    @NotNull(message = "<%= attr.name %> cannot be null")
<% } %>
<% if (attr.type === 'String' && !attr.nullable) { %>
    @NotBlank(message = "<%= attr.name %> cannot be blank")
<% } %>
<% if (attr.unique) { %>
    // Unique field: <%= attr.name %>
<% } %>
    private <%= attr.isRelation ? idType : attr.type %> <%= attr.name %><% if (attr.isRelation) { %>Id<% } %>;
<% } %>
<% }); %>
}
